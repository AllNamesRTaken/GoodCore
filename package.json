{
  "name": "goodcore",
  "version": "0.0.5",
  "description": "Some core functions and helper classes. List, Dictionary, Stack, Vec2, Rect, Range2, Tree. Optimized for speed and memory efficiency. Also an object pool a few mixins and Helpers for Dom, Array and Object manipulation + general",
  "main": "./dist/lib/goodcore.umd.min.js",
  "typings": "./dist/lib/goodcore.d.ts",
  "files": [
    "dist/lib/goodcore.umd.min.js",
    "dist/lib/goodcore.d.ts"
  ],
  "scripts": {
    "install_tools": "yarn install && typings install",
    "clean": "rimraf ./dist",
    "clean-test": "rimraf ./dist/test",
    "tsc": "tsc",
    "umd": "node browserify-umd.js",
    "minify": "node minify-umd.js",
    "build": "yarn run clean && yarn run tsc && yarn run umd && yarn run minify && yarn run bundle_d_ts",
    "build_test": "yarn run clean-test && tsc -p tsconfig-test.json",
    "test": "yarn run tsc && mocha ./dist/test/*Test.js",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- ./dist/test/*.js",
    "bundle_d_ts": "node dts-bundle.js",
    "publish_to_yarn": "yarn publish",
    "all": "yarn run build && yarn run test && yarn run cover"
  },
  "dependencies": {
    "ts-md5": "^1.2.0"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "browserify-umdify": "^1.0.3",
    "chai": "^3.5.0",
    "dts-bundle": "^0.7.2",
    "glob": "^7.0.6",
    "istanbul": "^0.4.5",
    "jsdom": "^9.12.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.2",
    "typescript": "^2.2.2",
    "typings": "^2.1.0",
    "uglify-js": "^2.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/allnamesrtaken/goodcore.git"
  },
  "keywords": [
    "Typescript",
    "Javascript"
  ],
  "author": "Joel Brage",
  "license": "MIT"
}
