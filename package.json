{
  "name": "goodcore",
  "version": "3.3.0",
  "description": "Some core functions and helper classes. List, Dictionary, Stack, Vec2, Rect, Range2, Tree. Optimized for speed and memory efficiency. Also an object pool a few mixins and Helpers for Dom, Array and Object manipulation + general",
  "main": "index.js",
  "typings": "index.d.ts",
  "unpkg": "goodcore.bundle.js",
  "files": [
    "*"
  ],
  "type": "module",
  "scripts": {
    "bench": "tsc -p tsconfig.bench.json && node ./dist/benchmark/index.js",
    "clean": "rimraf ./dist",
    "coverage": "vitest run --coverage",
    "deploy": "gulp package && npm publish dist\\lib",
    "dev": "vite",
    "build": "npm run clean && npm run build-bundle",
    "build-bundle": "vite build --config vite-full-bundle.config.js",
    "build-es": "vite build --config vite-full-es.config.js",
    "build-lite": "vite build --config vite-lite.config.js",
    "build-all": "npm run clean && tsc -p tsconfig.full.json && npm run build-lite && npm run build-bundle && npm run build-es && gulp copyDTS && gulp copyWebpackExternalsFn",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest - watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.4",
    "@babel/plugin-external-helpers": "^7.27.1",
    "@babel/plugin-transform-member-expression-literals": "^7.27.1",
    "@babel/plugin-transform-property-literals": "^7.27.1",
    "@babel/plugin-transform-property-mutators": "^7.27.1",
    "@babel/plugin-transform-reserved-words": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@types/benchmark": "^2.1.5",
    "@types/jsdom": "^21.1.7",
    "@types/node": "24.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "benchmark": "2.1.4",
    "chalk": "5.4.1",
    "eslint": "9.29.0",
    "gulp": "5.0.1",
    "gulp-rename": "2.0.0",
    "jsdom": "26.1.0",
    "prettier": "3.6.1",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}