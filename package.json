{
  "name": "goodcore",
  "version": "0.0.20",
  "description": "Some core functions and helper classes. List, Dictionary, Stack, Vec2, Rect, Range2, Tree. Optimized for speed and memory efficiency. Also an object pool a few mixins and Helpers for Dom, Array and Object manipulation + general",
  "main": "./dist/goodcore.umd.min.js",
  "typings": "./dist/goodcore.d.ts",
  "files": [
    "dist/goodcore.umd.min.js",
    "dist/goodcore.d.ts",
    "dist/lib/"
  ],
  "scripts": {
    "install_tools": "yarn install && typings install",
    "clean": "rimraf ./dist",
    "clean-test": "rimraf ./dist/test",
    "tsc": "tsc",
    "rollup": "rollup -c",
    "rollup_test": "rollup -c rollup_test.config.js",
    "minify": "node minify-umd.js",
    "build": "yarn run clean && tsc && yarn run rollup && yarn run minify && yarn run bundle_d_ts",
    "build_test": "yarn run clean-test && tsc -p tsconfig-test.json",
    "test": "yarn build_test && mocha ./dist/test/*Test.js",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- ./dist/test/*.js",
    "bundle_d_ts": "node dts-bundle.js",
    "finish_up": "node finish-up.js",
    "publish_to_npm": "npm publish",
    "all": "yarn run build && yarn run test && yarn run cover"
  },
  "dependencies": {
    "ts-md5": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "dts-bundle": "^0.7.2",
    "istanbul": "^0.4.5",
    "jsdom": "^9.12.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "rollup-plugin-typescript": "^0.8.1",
    "source-map-support": "^0.4.2",
    "typescript": "^2.3.2",
    "typings": "^2.1.0",
    "uglify-js": "^2.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/allnamesrtaken/goodcore.git"
  },
  "keywords": [
    "Typescript",
    "Javascript"
  ],
  "author": "Joel Brage",
  "license": "MIT"
}
